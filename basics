class Zespolone_Tyczynski{
        double a,b;


        Zespolone_Tyczynski(double x, double y){
                a=x;
                b=y;
        }
        double rzeczywista(){
                return a;
        }
        double zespolona(){
                return b;
        }
        double modul(){
                return Math.sqrt(a*a+b*b);
                }
        double przeciwna(){
                return ((-b)-a);
        }
        Zespolone_Tyczynski suma(Zespolone_Tyczynski ob){
                return new Zespolone_Tyczynski(a+ob.a,b+ob.b);
                }
        Zespolone_Tyczynski roznica(Zespolone_Tyczynski ob){
                return new Zespolone_Tyczynski(a-ob.a,b-ob.b);
                }
        Zespolone_Tyczynski iloczyn(Zespolone_Tyczynski ob){
                return new Zespolone_Tyczynski((a*ob.a-b*ob.b),(a*ob.a+b*ob.b));
                }
        Zespolone_Tyczynski iloraz(Zespolone_Tyczynski ob){
                return new Zespolone_Tyczynski((a*ob.a+b*ob.b)/(ob.modul()*ob.modul()),(a*ob.a-b*ob.b)/(ob.modul()*ob.modul()));
                }
        Zespolone_Tyczynski pierwiastek(){
                double fi=Math.atan2(a,b);
                return new Zespolone_Tyczynski(Math.sqrt(modul())*Math.cos((fi+2*Math.PI)/2),Math.sqrt(modul())*Math.sin((fi+2*Math.PI)/2));
        }
        public String toString(){
                return ""+a+"+i"+b;
                }
                }

